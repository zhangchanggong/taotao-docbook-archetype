<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0 http://maven.apache.org/xsd/archetype-descriptor-1.1.0.xsd" name="taotao-docbook"
    xmlns="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <requiredProperties>
    <requiredProperty key="name">
      <defaultValue>示例文档</defaultValue>
    </requiredProperty>
  </requiredProperties>
  <fileSets>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>src/main/docbook</directory>
      <includes>
        <include>**/*.xml</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>src/main/fonts</directory>
      <includes>
        <include>**/*.md</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>src/main/style</directory>
      <includes>
        <include>**/*.md</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>src/main/xslt</directory>
      <includes>
        <include>**/*.xsl</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>src/main/docx</directory>
      <includes>
        <include>**/*.md</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>src/main/plantuml</directory>
      <includes>
        <include>**/*.puml</include>
      </includes>
    </fileSet>
  </fileSets>
</archetype-descriptor>
